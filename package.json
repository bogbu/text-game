{
	"name": "text-game-beta",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"description": "Modern SvelteKit + Electron + Typescript + Tailwind Boilerplate.",
	"scripts": {
		"dev": "concurrently -k \" vite dev\" \" npm --prefix ./src-electron run dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"autoprefixer": "^10.4.20",
		"concurrently": "^9.0.1",
		"electron": "^32.1.2",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"tailwindcss": "^3.4.13",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8"
	},
	"dependencies": {
		"@sveltejs/adapter-vercel": "^5.4.4",
		"svelte-preprocess": "^6.0.2"
	}
}
